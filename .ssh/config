Include local_config

Host dz
    HostName dimensionzero.net
    IdentityFile ~/.ssh/zeromaster.key
    ForwardAgent yes
    # Headless vps
    ForwardX11 no

Host zn zeronet
    HostName zeronet.dimensionzero.net
    IdentityFile ~/.ssh/zeromaster.key
    ForwardAgent yes
    ForwardX11 yes

Host oh oht scrumble orange
    HostName orangehattech.com
    IdentityFile ~/.ssh/orangehattech.key

Host github
    User git
    Hostname github.com
    IdentityFile ~/.ssh/github.key

Host prpc
    Hostname 104.237.203.33
    IdentityFile ~/.ssh/zeromaster.key
    ForwardAgent yes

Host *
    User zeroknight
    ForwardAgent no
    VerifyHostKeyDNS yes
    ControlMaster auto
    ControlPath /tmp/ssh_mux_zeroknight_%r@%h:%p
    ServerAliveInterval 120
